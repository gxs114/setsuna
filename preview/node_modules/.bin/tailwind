#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/guxiansheng/Desktop/框架/setsuna-core-0.3/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/guxiansheng/Desktop/框架/setsuna-core-0.3/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/tailwindcss@3.1.8_postcss@8.4.16/node_modules/tailwindcss/lib/cli.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/tailwindcss@3.1.8_postcss@8.4.16/node_modules/tailwindcss/lib/cli.js" "$@"
fi
